{
  "data_path": "../data/output/metadata/image_metadata.csv",
  "pipeline arguments": {
    "delete_empty_columns": {
      "threshold": 0.90395480226,
      "delete_by": ["Unknown (0)"],
      "include": null,
      "exclude": null
    },
    "drop_columns": {
      "columns": ["Directory", "FocalLength35efl", "GPSDateTime", "GPSPosition"]
    },
    "set_index_from_column": {
      "index_column": "FileName",
      "index_name": "FileName"
    },
    "bring_up_measure_units": {
      "include": null,
      "exclude": null
    },
    "convert_datetime": {
      "regex_from": ["_d{4}:_d{2}:_d{2} (_d{2}:_d{2}:_d{2})?(_+_d{2}:_d{2})?"],
      "include": null,
      "exclude": null
    },
    "convert_fraction_columns_to_float": {
      "include": null,
      "exclude": null
    },
    "split_string_column": {
      "column_name": "ImageSize",
      "new_column_names": ["ImageWidth", "ImageHeight"],
      "keep": false,
      "separator": "x"
    },
    "gps_dms_to_dd": {
        "include": null,
        "exclude": null
    },
    "modify_column": {
      "column_name": "'YCbCrSubSampling'",
      "possible_prefixes": ["YCbCr"],
      "possible_suffixes": ["_(_d+ _d+_)"],
      "bracket_content": null
    },
    "remove_prefix": {
      "column_name": "MIMEType",
      "prefix": "image/"
    }
  }
}
